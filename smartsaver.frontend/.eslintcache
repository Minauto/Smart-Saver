s';\r\nimport { useDispatch } from 'react-redux';\r\n\r\nexport default ({ expense, setIsEditing }) => {\r\n    const descriptions = ['Groceries', 'Credit Card', 'Bills', 'Eating out', 'Gas']\r\n    const [amount, setAmount] = useState(0);\r\n    const [description, setDescription] = useState(descriptions[0]);\r\n    const [isNewExpense, setIsNewExpense] = useState(true);\r\n    const dispatch = useDispatch();\r\n\r\n    useEffect(() => {\r\n        if(expense !== undefined) {\r\n            setIsNewExpense(false);\r\n            setAmount(expense.amount);\r\n        }\r\n        else {\r\n            setIsNewExpense(true);\r\n        }\r\n    }, [expense]);\r\n\r\n    return <Form\r\n        onSubmit={event => {\r\n            event.preventDefault();\r\n            if (isNewExpense) {\r\n                NewExpense(dispatch, { description: description, amount: Number(amount) });\r\n            }\r\n            else {\r\n                EditExpense(dispatch, {id: expense.id, description: description, amount: Number(amount) });\r\n                setIsEditing(false);\r\n            }\r\n        }}\r\n    >\r\n        <Row>\r\n            <Col>\r\n                <Form.Label>Description</Form.Label>\r\n                <Form.Control as='select'\r\n                    onChange={event => setDescription(event.target.value)}>\r\n                    {descriptions.map(d => <option>{d}</option>)}\r\n                </Form.Control>\r\n            </Col>\r\n            <Col>\r\n                <Form.Label>Amount</Form.Label>\r\n                <Form.Control type='number' step='0.01'\r\n                    placeholder={amount}\r\n                    onChange={event => setAmount(event.target.value)} />\r\n            </Col>\r\n            <div style={{ marginTop: 'auto' }}>\r\n                {isNewExpense\r\n                    ? <Button variant='primary' type='submit'>Add</Button>\r\n                    : <div>\r\n                        <Button style={{ marginRight: '2px'}} variant='danger'\r\n                        onClick={() => DeleteExpense(dispatch, expense)}>Delete</Button>\r\n                        <Button style={{ marginRight: '2px'}} variant='success' type='submit'>Save</Button>\r\n                        <Button style={{ marginRight: '2px'}} variant='default' onClick={() => setIsEditing(false)}>Cancel</Button>\r\n                    </div>}\r\n            </div>\r\n        </Row>\r\n    </Form>\r\n}","C:\\Users\\PC\\source\\repos\\Smart-Saver\\smartsaver.frontend\\src\\components\\ExpenseList.js",["56","57"],"import { React, useEffect, useState } from 'react';\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport { GetExpenses } from '../services/expenses';\r\nimport { Button, Row, Col } from 'react-bootstrap';\r\nimport ExpenseForm from './ExpenseForm';\r\n\r\nexport default () => {\r\n    const dispatch = useDispatch();\r\n    const expenses = useSelector(state => state.expensesReducer.expenses);\r\n\r\n    useEffect(() => {\r\n        GetExpenses(dispatch);\r\n    }, []);\r\n\r\n    return expenses.map(e =>\r\n        <div key={e.id} style={{ marginBottom: '1rem' }}>\r\n            <ListRow expense={e} />\r\n        </div>\r\n    );\r\n}\r\n\r\nconst ListRow = ({ expense }) => {\r\n    const [isEditing, setIsEditing] = useState(false);\r\n    return isEditing\r\n        ? <ExpenseForm expense={expense} setIsEditing={setIsEditing} />\r\n        : <div>\r\n            <Row>\r\n                <Col>{expense.description}</Col>\r\n                <Col>â‚¬{expense.amount}</Col>\r\n                <Button variant=\"warning\" onClick={ () => setIsEditing(!isEditing)}>Edit</Button>\r\n            </Row>\r\n            <hr />\r\n        </div>\r\n}","C:\\Users\\PC\\source\\repos\\Smart-Saver\\smartsaver.frontend\\src\\app\\expensesReducer.js",["58","59"],"const initialState = {\r\n    expenses: [],\r\n}\r\n\r\nexport const ActionTypes = {\r\n    SET_EXPENSES: 'SET_EXPENSES',\r\n    NEW_EXPENSE: 'NEW_EXPENSE',\r\n    EDIT_EXPENSE: 'EDIT_EXPENSE',\r\n    DELETE_EXPENSE: 'DELETE_EXPENSE',\r\n}\r\n\r\nexport const ActionCreators = {\r\n    setExpenses: payload => ({ type: ActionTypes.SET_EXPENSES, payload }),\r\n    newExpense: payload => ({ type: ActionTypes.NEW_EXPENSE, payload }),\r\n    editExpense: payload => ({ type: ActionTypes.EDIT_EXPENSE, payload }),\r\n    deleteExpense: payload => ({ type: ActionTypes.DELETE_EXPENSE, payload }),\r\n\r\n}\r\n\r\nexport default (state = initialState, action) => {\r\n    switch (action.type) {\r\n        case ActionTypes.SET_EXPENSES:\r\n            return { ...state, expenses: [...action.payload] };\r\n        case ActionTypes.NEW_EXPENSE:\r\n            return { ...state, expenses: [action.payload, ...state.expenses] }\r\n        case ActionTypes.EDIT_EXPENSE:\r\n            var expenses = state.expenses.map(expense => {\r\n                if (expense.id === action.payload.id) {\r\n                    expense = action.payload;\r\n                }\r\n                return expense;\r\n            });\r\n            return { ...state, expenses: [...expenses] };\r\n        case ActionTypes.DELETE_EXPENSE:\r\n            var expenses = state.expenses.filter(expense =>\r\n                expense.id !== action.payload.id);\r\n            return { ...state, expenses: [...expenses] };\r\n        default:\r\n            return state;\r\n    }\r\n}","C:\\Users\\PC\\source\\repos\\Smart-Saver\\smartsaver.frontend\\src\\services\\expenses.js",[],"C:\\Users\\PC\\source\\repos\\Smart-Saver\\smartsaver.frontend\\src\\components\\auth\\SignIn.js",["60"],"C:\\Users\\PC\\source\\repos\\Smart-Saver\\smartsaver.frontend\\src\\components\\pages\\ExpensesPage.js",["61","62","63"],"C:\\Users\\PC\\source\\repos\\Smart-Saver\\smartsaver.frontend\\src\\components\\auth\\SignUp.js",["64"],{"ruleId":"65","severity":1,"message":"66","line":9,"column":8,"nodeType":"67","messageId":"68","endLine":9,"endColumn":14},{"ruleId":"65","severity":1,"message":"69","line":10,"c